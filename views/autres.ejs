<body ng-app="bv"  layout="column" >
<div ui-view="header"></div>
<div >
    <div layout="column"  flex class="Login">
        <div flex="95" layout="row" class="Login-content">
            <div  flex="55"  flex-offset="15" class="tehc"   hide-sm hide-xs >
                <h5  class="Login-axgrip">
                    AXGRIP
                </h5>
                <p class="Login-description">
                    Des milliers d'utilisateurs <br> classes selon les profils <br> et les motivations communes...
                </p>
            </div>
            <div   flex="25"  flex-sm="100" flex-xs="100" layout="column" class="Login-above-content">

                <div flex="20"  class="header"  layout="row" layout-align="center center">
                    <div  ><img src="../../image/GRIP_Logo.png" alt="" class="Login-image" ></div>
                </div>


                <div flex="60" ng-show="affiche" class="content" class="md-padding" >

                        <md-icon md-svg-src="../../image/left-arrow-in-circular-button.svg" ng-click="newUser2()"
                                 aria-label="android " style="color: blue;" ></md-icon>
                        <form  name="myForm"  ng-submit="updatepassword()">
                            <md-content class="md-no-momentum newuser">
                                <md-input-container md-no-float class="md-block">
                                    <md-icon md-svg-src="../../image/lock-and-key-icon-silhouette.svg"></md-icon>
                                    <input ng-model="user.password" type="password" placeholder="Nouveau mot de passe (required)" ng-required="true">
                                </md-input-container>
                                <md-input-container md-no-float class="md-block">
                                    <md-icon md-svg-src="../../image/lock-and-key-icon-silhouette.svg"></md-icon>
                                    <input ng-model="user.password" type="password" placeholder="Confirm Nouveau mot de passe (required)" ng-required="true">
                                </md-input-container>
                                <md-button class="md-raised md-primary" type="submit">Modifier mot de passe</md-button>
                            </md-content>
                        </form>

                </div>
            </div>
        </div>
    </div>
</div>
